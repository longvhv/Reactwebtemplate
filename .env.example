# API Gateway Configuration
VITE_API_GATEWAY_URL=http://localhost:8000

# Service Information
VITE_SERVICE_NAME=react-frontend
VITE_SERVICE_VERSION=1.0.0

# Environment
VITE_ENV=development

# Feature Flags (optional)
VITE_ENABLE_SERVICE_DISCOVERY=false
VITE_ENABLE_MONITORING=true
VITE_ENABLE_LOGGING=true

# API Endpoints (optional - if not using service discovery)
VITE_AUTH_SERVICE_URL=http://localhost:8001
VITE_USER_SERVICE_URL=http://localhost:8002
VITE_PROFILE_SERVICE_URL=http://localhost:8003

# Timeout Configuration (optional)
VITE_API_TIMEOUT=30000

# Logging Configuration (optional)
VITE_LOG_LEVEL=info
